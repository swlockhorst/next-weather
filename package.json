    "axios": "^0.21.1",
    "emotion-normalize": "^11.0.1",
    "framer-motion": "^3.3.0",
    "next": "latest",
    "nuka-carousel": "^4.7.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.1",
    "styled-normalize": "^8.0.7",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.2.0",
